<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.quizapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.android.tools.RobotsBackground"
            android:label="@string/robots_background" />
        <activity
            android:name="com.android.tools.RobotsHistory"
            android:label="@string/robots_history" />
        <activity
            android:name="com.android.tools.RobotsAndmaths"
            android:label="@string/robots_maths" />
        <activity
            android:name="com.android.tools.RobotsAndTech"
            android:label="@string/robots_tech" />
        <activity
            android:name="com.android.tools.RobotsAndSociety"
            android:label="@string/robots_society">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
    </application>

</manifest>